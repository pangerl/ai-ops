{
  "mcpServers": {
    "fetch": {
      "disabled": true,
      "timeout": 60,
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "kubernetes": {
      "disabled": true,
      "command": "npx",
      "args": [
        "-y",
        "kubernetes-mcp-server@latest"
      ]
    },
    "victoriametrics": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "/Users/lanpang/data/mcp/mcp-victoriametrics",
      "env": {
        "VM_INSTANCE_ENTRYPOINT": "http://10.229.1.4:8428",
        "VM_INSTANCE_TYPE": "single",
        "VM_INSTANCE_BEARER_TOKEN": ""
      }
    },
    "cls-mcp-server": {
      "disabled": true,
      "name": "cls-mcp-server",
      "type": "stdio",
      "registryUrl": "",
      "command": "npx",
      "args": [
        "-y",
        "cls-mcp-server"
      ],
      "env": {
        "TRANSPORT": "stdio",
        "TENCENTCLOUD_SECRET_ID": "YOUR_TENCENT_SECRET_ID",
        "TENCENTCLOUD_SECRET_KEY": "YOUR_TENCENT_SECRET_KEY",
        "TENCENTCLOUD_API_BASE_HOST": "tencentcloudapi.com",
        "TENCENTCLOUD_REGION": "ap-guangzhou",
        "MAX_LENGTH": "15000"
      }
    }
  }
}
